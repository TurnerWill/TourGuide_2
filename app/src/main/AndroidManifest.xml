        android:theme="@style/AppTheme">

        <meta-data android:name="OnlineMaps.Key" android:value="DZSGtCBRXs2794pxiaAywmsQwxQ9gLn6"/>
        <meta-data android:name="OnlineSearch.Key" android:value="DZSGtCBRXs2794pxiaAywmsQwxQ9gLn6"/>
        <meta-data android:name="OnlineRouting.Key" android:value="DZSGtCBRXs2794pxiaAywmsQwxQ9gLn6"/>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".FlowerActivity"></activity>

        <activity android:name=".ParkActivity"></activity>

        <activity android:name=".SchoolActivity"></activity>

        <activity android:name=".RestaurantActivity"></activity>

        <activity android:name=".DetailActivity"
